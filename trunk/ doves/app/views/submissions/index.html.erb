

<h1>Listing submissions</h1>

<table>
  <tr>
    <th>S degree</th>
    <th>Sub date time</th>
    <th>User</th>
    <th>Sub fname</th>
    <th>Sub lname</th>
    <th>Common name</th>
    <th>Num of birds</th>
    <th>Age</th>
    <th>Sex</th>
    <th>Plumage</th>
    <th>Location</th>
    <th>Region</th>
    <th>Sight date time</th>
    <th>Habitat</th>
    <th>Coobservers</th>
    <th>Length of obs</th>
    <th>Distance from</th>
    <th>Detailed description</th>
    <th>Detailed behavior</th>
    <th>Distinguished char</th>
    <th>Prev bird exp</th>
    <th>Optical equipment</th>
    <th>References</th>
    <th>Sub recall</th>
    <th>Guide use</th>
    <th>Unusual</th>
    <th>Notes</th>
    <th>Media</th>
    <th>Status</th>
    <th>Rounds</th>
    <th>Date accepted</th>
    <th>Date votable</th>
    <th>Admin notes</th>
    <th>Working notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @submissions.each do |submission| %>
  <tr>
    <td><%= submission.s_degree %></td>
    <td><%= submission.created_on%></td>
    <td><%= submission.user_id %></td>
    <td><%= submission.user.first_name%></td>
    <td><%= submission.user.last_name %></td>
    <td><%= submission.bird.common_name %>
		<% if submission.bird_id == 31 %>
		(<%= submission.common_name %>)
		<% end %></td>
    <td><%= submission.num_of_birds %></td>
    <td><%= submission.age %></td>
    <td><%= submission.sex %></td>
    <td><%= submission.plumage %></td>
    <td><%= submission.location %></td>
    <td><%= submission.region %></td>
    <td><%= submission.sight_date_time %></td>
    <td><%= submission.habitat %></td>
    <td><%= submission.coobservers %></td>
    <td><%= submission.length_of_obs %></td>
    <td><%= submission.distance_from %></td>
    <td><%= submission.detailed_description %></td>
    <td><%= submission.detailed_behavior %></td>
    <td><%= submission.distinguished_char %></td>
    <td><%= submission.prev_bird_exp %></td>
    <td><%= submission.optical_equipment %></td>
    <td><%= submission.references %></td>
    <td><%= submission.sub_recall %></td>
    <td><%= submission.guide_use %></td>
    <td><%= submission.unusual %></td>
    <td><%= submission.notes %></td>
    <td><%= submission.media %></td>
    <td><%= submission.status %></td>
    <td><%= submission.rounds %></td>
    <td><%= submission.date_accepted %></td>
    <td><%= submission.date_votable %></td>
    <td><%= submission.admin_notes %></td>
    <td><%= submission.working_notes %></td>
    <td><%= link_to 'Show', submission %></td>
    <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
    <td><%= link_to 'Destroy', submission, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />
<%= will_paginate @submissions %>
<%= link_to 'New Submission', new_submission_path %>
