<h1>Listing multimedia</h1>

<table>
  <tr>
    <th>Submission</th>
    <th>Type</th>
    <th>Time</th>
    <th>Size</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @multimedia.each do |multimedium| %>
  <tr>
    <td><%= multimedium.Submission_id %></td>
    <td><%= multimedium.type %></td>
    <td><%= multimedium.time %></td>
    <td><%= multimedium.size %></td>
    <td><%= link_to 'Show', multimedium %></td>
    <td><%= link_to 'Edit', edit_multimedium_path(multimedium) %></td>
    <td><%= link_to 'Destroy', multimedium, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

